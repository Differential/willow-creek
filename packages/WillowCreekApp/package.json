    "prettier": "1.15.2",
    "prop-types": "^15.6.2",
    "querystring": "^0.2.0",
    "react": "16.6.3",
    "react-apollo": "2.3.1",
    "react-dom": "^16.6.3",
    "react-native": "0.57.5",
    "react-native-custom-tabs": "0.1.7",
    "react-native-device-info": "0.24.3",
