    "apollo-link-schema": "1.1.1",
    "apollo-link-state": "0.4.2",
    "apollo-upload-client": "9.1.0",
    "buffer": "^5.2.1",
    "color": "3.1.0",
    "entities": "1.1.2",
    "events": "^3.0.0",
    "formik": "0.11.11",
    "graphql": "0.13.2",
    "graphql-tag": "2.10.0",
    "graphql-tools": "3.1.1",
    "htmlparser2": "3.10.0",
    "lodash": "^4.17.11",
