    "rn-placeholder": "1.3.0",
    "yup": "0.26.6"
  },
  "devDependencies": {
    "babel-jest": "23.6.0",
    "get-dev-paths": "^0.1.0",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.49.2",
    "react-test-renderer": "16.6.3"
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test|tests).{js,jsx,mjs}"
